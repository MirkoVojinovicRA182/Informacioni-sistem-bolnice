        <ScrollViewer HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792" VerticalScrollBarVisibility="Hidden">
            <Label Content="1. Da li Vas je lekar pažljivo slušao?&#xA;&#xA;2. Da li se lekar prema Vama ophodio sa ljubaznošću i poštovanjem?&#xA;&#xA;3. Da li Vam je lekar dao jasna i precizna objašnjenja?&#xA;&#xA;4. Da li Vam je lekar dao korisne savete?&#xA;&#xA;&#xA;&#xA;&#xA;Ocena:&#xA;" FontSize="20"/>
        </ScrollViewer>
        <Button Content="Slanje ankete" HorizontalAlignment="Left" VerticalAlignment="Top" Width="164" Margin="282,374,0,0" Height="35" Click="Button_Click"/>
        <ComboBox x:Name="comboBoxQuestion1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="30,36,0,0"/>
        <ComboBox x:Name="comboBoxQuestion2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="30,90,0,0"/>
        <ComboBox x:Name="comboBoxQuestion3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="30,142,0,0"/>
        <ComboBox x:Name="comboBoxQuestion4" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="30,196,0,0"/>
        <ComboBox x:Name="comboBoxRating" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="82,303,0,0"/>
    </Grid>
</Window>
